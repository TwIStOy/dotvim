{
  "mcpServers": {
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "git": {
      "args": [
        "mcp-server-git"
      ],
      "command": "uvx",
      "disabled": false,
      "env": {
        "UV_PYTHON": "${cmd: which python3}"
      }
    },
    "github": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "command": "npx",
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${cmd:cat /run/agenix/github-cli-access-token}"
      }
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": false
    }
  }
}
