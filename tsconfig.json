{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "types": [],
    "strict": true,
    "noImplicitAny": true,
    "outDir": "lua/tsgen",
  },
  "tstl": {
    "luaTarget": "JIT",
    "sourceMapTraceback": true,
  }
}
